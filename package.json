{
  "name": "cp2-shift-dms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-seed-all": "cross-env NODE_ENV=test sequelize db:seed:undo:all && cross-env NODE_ENV=test sequelize db:seed:all",
    "test-server": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/test/**/*.js --timeout 90000",
    "test-client": "cross-env NODE_ENV=test jest",
    "test": "npm run test-server && npm run test-client",
    "dev": "nodemon --watch server ./server/bin/www --exec babel-node",
    "prestart": "npm run build",
    "start": "node ./dist/bin/www",
    "build": "rimraf ./dist && babel ./server -d ./dist --copy-files"
  },
  "jest": {
    "moduleFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/server/utils/",
      "/test-globals.js"
    ],
    "collectCoverage": true,
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react"
    ],
    "setupFiles": [
      "<rootDir>/test-globals.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-ifatoki/CP2-Shift-DMS.git"
  },
